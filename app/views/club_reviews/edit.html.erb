<h1>ClubReviews#edit</h1>
<p>Find me in app/views/club_reviews/edit.html.erb</p>

<%= simple_form_for [current_user, @club, @club_review] do |f|  %>
  <%= f.error_notification %>
  <%= f.input :rating, collectioon: [1, 2, 3, 4, 5] %>
  <%= f.input :comment %>
  <%= f.submit "Edit review", class: "btn btn-success"%>
<% end %>
